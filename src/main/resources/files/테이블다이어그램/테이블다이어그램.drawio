Table DEPOSIT_INFO {
  ACCOUNT_NO varchar(20) [primary key]
  ACCOUNT_ST_CD varchar(2)
  ACCOUNT_TYPE_CD varchar(2)
  CUSTOMER_NO number
  OPEN_DT date
  MATURITY_DT date
  CURRENT_BAL number(15, 2)
  ACCOUNT_DESC varchar(255)
  INTEREST_RATE number(5, 2)
  FEE_EXEMPTION_FLAG varchar(1)
}

Table DEPOSIT_TRSC_HIS {
  ACCOUNT_NO varchar(20)
  TRSC_SEQ_NO number
  TRSC_ST_CD varchar(2)
  TRSC_DT date
  TRSC_TM varchar(8)
  REAL_PROC_DT date
  REAL_PROC_TM varchar(8)
  CANCEL_DT date
  CANCEL_TM varchar(8)
  CANCEL_TRSC_SEQ_NO number
  TRANSFER_PROC_CD varchar(2)
  DEBIT_CREDIT_CD varchar(2)
  TRSC_TYPE_CD varchar(2)
  TRSC_CURRENCY_CD varchar(3)
  BF_TRSC_BAL number(15, 2)
  TRSC_AMT number(15, 2)
  AF_TRSC_BAL number(15, 2)
  FEE_AMT number(15, 2)
}

Table TRSC_RELATION {
  ORG_TRSC_ID varchar(20) [primary key]
  ORG_TRSC_SEQ_NO number [primary key]
  ORG_ASSIGN_CD varchar(2)
  TRSC_TYPE_CD varchar(2)
  REL_TRSC_ID varchar(20)
  REL_TRSC_SEQ_NO number
  REL_ASSIGN_CD varchar(2)
}

Table BANK_CUSTOMER {
  CUSTOMER_NO varchar(20) [primary key]
  CUSTOMER_ST_CD varchar(2)
  CUSTOMER_TYPE_CD varchar(2)
  ID_CARD_TYPE_CD varchar(2)
  ID_CARD_NO varchar(20)
  CUSTOMER_NM varchar(100)
  KAKAO_NO varchar(20)
  EMAIL_ADDRESS varchar(100)
  MOBILE_PHONE_NO varchar(20)
  PRIVACY_AGREE_FLAG varchar(1)
  EMAIL_AGREE_FLAG varchar(1)
  SMS_AGREE_FLAG varchar(1)
  PUSH_AGREE_FLAG varchar(1)
  KAKAO_AGREE_FLAG varchar(1)
}

Table KAKAO_CUSTOMER {
  KAKAO_NO varchar(20) [primary key]
  KAKAO_ID varchar(100)
  KAKAO_ST_CD varchar(2)
  KAKAO_TYPE_CD varchar(2)
  ID_CARD_TYPE_CD varchar(2)
  ID_CARD_NO varchar(20)
  KAKAO_NM varchar(100)
  EMAIL_ADDRESS varchar(100)
  MOBILE_PHONE_NO varchar(20)
  KAKAO_BANK_CUST_FLAG varchar(1)
  PRIVACY_AGREE_FLAG varchar(1)
  EMAIL_AGREE_FLAG varchar(1)
  SMS_AGREE_FLAG varchar(1)
  PUSH_AGREE_FLAG varchar(1)
  KAKAO_AGREE_FLAG varchar(1)
}

Table KAKAO_CUSTOMER_TRSC_HIS {
  KAKAO_NO varchar(20)
  TRSC_SEQ_NO number
  TRSC_ST_CD varchar(2)
  TRSC_DT date
  TRSC_TM varchar(8)
  REAL_PROC_DT date
  REAL_PROC_TM varchar(8)
  CANCEL_DT date
  CANCEL_TM varchar(8)
  CANCEL_TRSC_SEQ_NO number
  TRANSFER_PROC_CD varchar(2)
  DEBIT_CREDIT_CD varchar(2)
  TRSC_CURRENCY_CD varchar(3)
  TRSC_AMT number(15, 2)
}

Table NOTIFICATION_TEMPLATE {
  TEMPLATE_ID varchar(20) [primary key]
  TEMPLATE_TYPE_CD varchar(2)
  TEMPLATE_BUSINESS_CD varchar(2)
  TEMPLATE_DESCRIPTION varchar(255)
  TEMPLATE_TITLE varchar(4000)
  TEMPLATE_MESSAGE_CONTENT clob
}

Table NOTIFICATION_QUEUE {
  NOTIFICATION_QUEUE_NO number [primary key]
  TEMPLATE_ID varchar(20)
  TEMPLATE_TYPE_CD varchar(2)
  RECIPIENTTYPE_CD varchar(2)
  RECIPIENT varchar(20)
  NOTIFICATION_REG_DTM timestamp
  SCHEDULED_SEND_DATETIME timestamp
  NOTIFICATION_TITLE_PARAM varchar(4000)
  NOTIFICATION_MESSAGE_PARAM varchar(4000)
}

Table NOTIFICATION_KAKAO_LOG {
  NOTIFICATION_DT date
  KAKAO_NO varchar(20)
  NOTIFICATION_SEQ_NO number
  NOTIFICATION_QUEUE_NO number
  NOTIFICATION_CREATE_DTM timestamp
  SCHEDULED_SEND_DTM timestamp
  REAL_SEND_DTM timestamp
  NOTIFICATION_TITLE varchar(4000)
  NOTIFICATION_MESSAGE clob
}

Table NOTIFICATION_CUSTOMER_LOG {
  NOTIFICATION_DT date
  CUSTOMER_NO varchar(20)
  NOTIFICATION_SEQ_NO number
  NOTIFICATION_QUEUE_NO number
  NOTIFICATION_CREATE_DTM timestamp
  SCHEDULED_SEND_DTM timestamp
  REAL_SEND_DTM timestamp
  NOTIFICATION_TITLE varchar(4000)
  NOTIFICATION_MESSAGE clob
}

Table NOTIFICATION_SUCCESS {
  NOTIFICATION_QUEUE_NO number [primary key]
  TEMPLATE_ID varchar(20)
  TEMPLATE_TYPE_CD varchar(2)
  RECIPIENTTYPE_CD varchar(2)
  RECIPIENT varchar(20)
  NOTIFICATION_CREATE_DTM timestamp
  NOTIFICATION_SUCCESS_DTM timestamp
  NOTIFICATION_TITLE_PARAM varchar(4000)
  NOTIFICATION_MESSAGE_PARAM varchar(4000)
}

Table NOTIFICATION_ERROR {
  NOTIFICATION_QUEUE_NO number [primary key]
  TEMPLATE_ID varchar(20)
  TEMPLATE_TYPE_CD varchar(2)
  RECIPIENTTYPE_CD varchar(2)
  RECIPIENT varchar(20)
  NOTIFICATION_CREATE_DTM timestamp
  NOTIFICATION_ERROR_DTM timestamp
  NOTIFICATION_ERROR_CNT number
  NOTIFICATION_TITLE_PARAM varchar(4000)
  NOTIFICATION_MESSAGE_PARAM varchar(4000)
  ERROR_LOG clob
}

Ref: DEPOSIT_TRSC_HIS.ACCOUNT_NO > DEPOSIT_INFO.ACCOUNT_NO
Ref: KAKAO_CUSTOMER_TRSC_HIS.KAKAO_NO > KAKAO_CUSTOMER.KAKAO_NO
Ref: NOTIFICATION_QUEUE.TEMPLATE_ID > NOTIFICATION_TEMPLATE.TEMPLATE_ID
Ref: NOTIFICATION_QUEUE.TEMPLATE_TYPE_CD > NOTIFICATION_TEMPLATE.TEMPLATE_TYPE_CD
Ref: NOTIFICATION_KAKAO_LOG.KAKAO_NO > KAKAO_CUSTOMER.KAKAO_NO
Ref: NOTIFICATION_KAKAO_LOG.NOTIFICATION_QUEUE_NO > NOTIFICATION_QUEUE.NOTIFICATION_QUEUE_NO
Ref: NOTIFICATION_CUSTOMER_LOG.CUSTOMER_NO > BANK_CUSTOMER.CUSTOMER_NO
Ref: NOTIFICATION_CUSTOMER_LOG.NOTIFICATION_QUEUE_NO > NOTIFICATION_QUEUE.NOTIFICATION_QUEUE_NO
